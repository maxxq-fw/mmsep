#################################################################
#    SWRL Rules
#################################################################

### Правило 1: Режиссер-Актёр
[ rdf:type swrl:Imp ;
  swrl:body [
      rdf:type swrl:AtomList ;
      rdf:first [
          rdf:type swrl:ClassAtom ;
          swrl:classPredicate :Актёр ;
          swrl:argument1 :person
      ] ;
      rdf:rest [
          rdf:type swrl:AtomList ;
          rdf:first [
              rdf:type swrl:ClassAtom ;
              swrl:classPredicate :Режиссер ;
              swrl:argument1 :person
          ] ;
          rdf:rest rdf:nil
      ]
  ] ;
  swrl:head [
      rdf:type swrl:AtomList ;
      rdf:first [
          rdf:type swrl:ClassAtom ;
          swrl:classPredicate :РежиссерАктёр ;
          swrl:argument1 :person
      ] ;
      rdf:rest rdf:nil
  ]
] .

### Правило 2: Актёр снимался в фильме
[ rdf:type swrl:Imp ;
  swrl:body [
      rdf:type swrl:AtomList ;
      rdf:first [
          rdf:type swrl:ClassAtom ;
          swrl:classPredicate :Актёр ;
          swrl:argument1 :actor
      ] ;
      rdf:rest [
          rdf:type swrl:AtomList ;
          rdf:first [
              rdf:type swrl:ClassAtom ;
              swrl:classPredicate :Фильм ;
              swrl:argument1 :movie
          ] ;
          rdf:rest [
              rdf:type swrl:AtomList ;
              rdf:first [
                  rdf:type swrl:IndividualPropertyAtom ;
                  swrl:propertyPredicate :играет_в_фильме ;
                  swrl:argument1 :actor ;
                  swrl:argument2 :movie
              ] ;
              rdf:rest rdf:nil
          ]
      ]
  ] ;
  swrl:head [
      rdf:type swrl:AtomList ;
      rdf:first [
          rdf:type swrl:ClassAtom ;
          swrl:classPredicate :АктёрСнималсяВФильме ;
          swrl:argument1 :actor
      ] ;
      rdf:rest rdf:nil
  ]
] .

### Правило 3: Фильм снят студией
[ rdf:type swrl:Imp ;
  swrl:body [
      rdf:type swrl:AtomList ;
      rdf:first [
          rdf:type swrl:ClassAtom ;
          swrl:classPredicate :Фильм ;
          swrl:argument1 :movie
      ] ;
      rdf:rest [
          rdf:type swrl:AtomList ;
          rdf:first [
              rdf:type swrl:ClassAtom ;
              swrl:classPredicate :Студия ;
              swrl:argument1 :studio
          ] ;
          rdf:rest [
              rdf:type swrl:AtomList ;
              rdf:first [
                  rdf:type swrl:IndividualPropertyAtom ;
                  swrl:propertyPredicate :производит ;
                  swrl:argument1 :studio ;
                  swrl:argument2 :movie
              ] ;
              rdf:rest rdf:nil
          ]
      ]
  ] ;
  swrl:head [
      rdf:type swrl:AtomList ;
      rdf:first [
          rdf:type swrl:ClassAtom ;
          swrl:classPredicate :ФильмСнятСтудией ;
          swrl:argument1 :movie
      ] ;
      rdf:rest rdf:nil
  ]
] .

### Правило 4: Актёр работал со студией
[ rdf:type swrl:Imp ;
  swrl:body [
      rdf:type swrl:AtomList ;
      rdf:first [
          rdf:type swrl:ClassAtom ;
          swrl:classPredicate :Актёр ;
          swrl:argument1 :actor
      ] ;
      rdf:rest [
          rdf:type swrl:AtomList ;
          rdf:first [
              rdf:type swrl:ClassAtom ;
              swrl:classPredicate :Фильм ;
              swrl:argument1 :movie
          ] ;
          rdf:rest [
              rdf:type swrl:AtomList ;
              rdf:first [
                  rdf:type swrl:ClassAtom ;
                  swrl:classPredicate :Студия ;
                  swrl:argument1 :studio
              ] ;
              rdf:rest [
                  rdf:type swrl:AtomList ;
                  rdf:first [
                      rdf:type swrl:IndividualPropertyAtom ;
                      swrl:propertyPredicate :играет_в_фильме ;
                      swrl:argument1 :actor ;
                      swrl:argument2 :movie
                  ] ;
                  rdf:rest [
                      rdf:type swrl:AtomList ;
                      rdf:first [
                          rdf:type swrl:IndividualPropertyAtom ;
                          swrl:propertyPredicate :производит ;
                          swrl:argument1 :studio ;
                          swrl:argument2 :movie
                      ] ;
                      rdf:rest rdf:nil
                  ]
              ]
          ]
      ]
  ] ;
  swrl:head [
      rdf:type swrl:AtomList ;
      rdf:first [
          rdf:type swrl:ClassAtom ;
          swrl:classPredicate :АктёрРаботалСоСтудией ;
          swrl:argument1 :actor
      ] ;
      rdf:rest rdf:nil
  ]
] .

### SWRL Variables
:person rdf:type swrl:Variable .
:actor rdf:type swrl:Variable .
:movie rdf:type swrl:Variable .
:studio rdf:type swrl:Variable .