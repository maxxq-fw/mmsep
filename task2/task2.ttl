@prefix : <http://www.semanticweb.org/maxim/ontologies/2025/9/untitled-ontology-11/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://www.semanticweb.org/maxim/ontologies/2025/9/untitled-ontology-11> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  Прямое отношение родитель-ребенок (НЕ транзитивное)
:имеет_родителя rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :является_потомком ;
                owl:inverseOf :является_родителем .

###  Обратное к имеет_родителя
:является_родителем rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :является_предком .

###  Транзитивное свойство "является потомком"
:является_потомком rdf:type owl:ObjectProperty ,
                            owl:TransitiveProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   owl:inverseOf :является_предком .

###  Транзитивное свойство "является предком"  
:является_предком rdf:type owl:ObjectProperty ,
                           owl:TransitiveProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/maxim/ontologies/2025/9/untitled-ontology-11/Человек
:Человек rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/maxim/ontologies/2025/9/untitled-ontology-11/Дед
:Дед rdf:type owl:NamedIndividual ,
              :Человек ;
     :является_родителем :Отец .

###  http://www.semanticweb.org/maxim/ontologies/2025/9/untitled-ontology-11/Дед_2
:Дед_2 rdf:type owl:NamedIndividual ,
                :Человек ;
       :является_родителем :Отец_2 .

###  http://www.semanticweb.org/maxim/ontologies/2025/9/untitled-ontology-11/Отец
:Отец rdf:type owl:NamedIndividual ,
               :Человек ;
      :является_родителем :Сын .

###  http://www.semanticweb.org/maxim/ontologies/2025/9/untitled-ontology-11/Отец_2
:Отец_2 rdf:type owl:NamedIndividual ,
                 :Человек ;
        :является_родителем :Сын_2 .

###  http://www.semanticweb.org/maxim/ontologies/2025/9/untitled-ontology-11/Прадед
:Прадед rdf:type owl:NamedIndividual ,
                 :Человек ;
        :является_родителем :Дед .

###  http://www.semanticweb.org/maxim/ontologies/2025/9/untitled-ontology-11/Прадед_2
:Прадед_2 rdf:type owl:NamedIndividual ,
                   :Человек ;
          :является_родителем :Дед_2 .

###  http://www.semanticweb.org/maxim/ontologies/2025/9/untitled-ontology-11/Сын
:Сын rdf:type owl:NamedIndividual ,
              :Человек .

###  http://www.semanticweb.org/maxim/ontologies/2025/9/untitled-ontology-11/Сын_2
:Сын_2 rdf:type owl:NamedIndividual ,
                :Человек .